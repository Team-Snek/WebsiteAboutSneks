@using WebsiteAboutSneks.Models;
@{
    ViewBag.Title = "Snakes";

}

@Html.ActionLink("Home", "Index", "Home")
> <span>Snakes</span>

<h2>All Our Snakes</h2>

<div class="container">
    <div class="row">
        <div class="col-sm-6">
            <ul>

                @foreach (Snake snek in ViewBag.SnakeList)
            {
                <li><a href="@Url.Action("ShowSnake", "Home", new { id = snek.snakeID })">@snek.snakeName</a></li>
        }
            </ul>
        </div>
        <div class="col-sm-6">
            <img class="img-responsive img-list" src="~/Content/images/monch.jpg" />
        </div>
    </div>
</div>

